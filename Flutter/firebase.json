{
  "hosting": {
    "appAssociation": "AUTO",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
    { "source": "/app/link/**", "dynamicLinks": true } 
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
